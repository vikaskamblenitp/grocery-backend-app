{
  "name": "sse-demo",
  "version": "1.0.0",
  "description": "server sent event demo",
  "main": "index.ts",
  "scripts": {
    "start:dev": "cross-env-shell NODE_ENV=development npx babel-node dist/src/index.js",
    "dev": "nodemon --watch src --watch lib --ext js,ts,json --exec \"npm run build && npm run start:dev\"",
    "nodemon": "nodemon --watch src --watch lib --ext js,ts,json --exec \"npm run build && npm run start\"",
    "start": "node dist/index.js",
    "prepare": "husky install",
    "lint": "eslint . --ext .js --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --write \"**/*.+(js|ts|json)\"",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json && babel src --out-dir dist",
    "build:clean": "rm -rf dist && npm run build",
    "build:watch": "tsc -w -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma:migrate && prisma:generate",
    "prisma:up": "prisma migrate dev",
    "prisma:down": "prisma migrate reset",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "db": "cross-env-shell db-migrate -m '$INIT_CWD/db/migrations'",
    "db:create": "npm run db db:create",
    "db:drop": "npm run db db:drop",
    "migrate:up": "npm run db up:migrate",
    "migrate:down": "npm run db down:migrate",
    "migrate:create": "npm run db create:migrate",
    "migrate:reset": "npm run db reset:migrate",
    "seed:up": "npm run db up:seed",
    "seed:down": "npm run db down:seed",
    "seed:create": "npm run db create:seed"
  },
  "keywords": [
    "nodejs",
    "sse"
  ],
  "author": "Vikas Kamble",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@prisma/client": "^5.22.0",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-useragent": "^1.0.15",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "pg": "^8.13.1",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@austinburns/type-guards": "^1.0.6",
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.9.7",
    "@types/babel__register": "^7.17.3",
    "@types/db-migrate-pg": "^0.0.13",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-vipul-base": "^1.2.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "imports": {
    "#configs/*": "./dist/src/configs/*",
    "#constants/*": "./dist/src/constants/*",
    "#helpers/*": "./dist/src/helpers/*",
    "#middlewares/*": "./dist/src/middlewares/*",
    "#utils/*": "./dist/src/utils/*",
    "#lib/*": "./lib/*",
    "#workers/*": "./src/workers/*"
  },
  "nodemonConfig": {
    "ignore": [
      "**/__tests__/**",
      "**/*.test.js"
    ],
    "delay": 2500
  }
}
